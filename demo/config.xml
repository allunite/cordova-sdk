<?xml version='1.0' encoding='utf-8'?>
<widget id="com.demo.allunitesdk" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>AlluniteSdkDemo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="com.allunite.sdk.cordova" spec="./..">
        <variable name="ACCOUNT_ID" value="CordovaDemo" />
        <variable name="ACCOUNT_KEY" value="CA16C4FE98CF47AAB7B56137E9E3D7C1" />
        <variable name="URI_SCHEME" value="all-unite-demo-cordova" />
    </plugin>
</widget>
